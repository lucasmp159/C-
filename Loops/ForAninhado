using System;

namespace teste
{
	public class test {
		public void testando(){
			float soma = 0;
			float fracao = 0;
			for (float n = 1; n <=3; n += 2)
				for (float s = 1; s <= 2; s++)
					fracao = (n/s);
					soma = fracao++;
			Console.WriteLine("A soma da fração dos números será: " + soma);
		}
	}
	public class Program{
				public static void Main(String[] args){
					test obj = new test();
					obj.testando();
			}
		}
	}
